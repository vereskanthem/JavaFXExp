<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane prefHeight="211.0" prefWidth="372.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddForm">
   <children>
       <TextFlow fx:id="addFormText" layoutX="30.0" layoutY="14.0" prefHeight="26.0" prefWidth="316.0" textAlignment="CENTER" />
      <Button fx:id="chooseButton" onAction="#chooseImageToAdd" layoutX="35.0" layoutY="58.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="302.0" text="Выбрать изображение" />
      <Button fx:id="addButton" onAction="#addImageToCategory" layoutX="250.0" layoutY="163.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="106.0" text="Добавить" />
      <ComboBox fx:id="categories" layoutX="35.0" layoutY="106.0" prefHeight="26.0" prefWidth="302.0" promptText="Категория" />
   </children>
</AnchorPane>
